import java.util.Random;
import java.util.Scanner;

public class practice3 {

	public static void main(String[] args) {
		String d="1";
		int d1=0;
		int[] dn= new int[5000];
		int gen=0;
		int sum=0;
		while(Integer.parseInt(d)<5000) {
			 gen=0;
			 gen+=Integer.parseInt(d);
			 for(int i=3; i>=0;i--) {
				 int k=Integer.parseInt(d);
				 if(k>0) {
					 gen+=k%10;
					 k/=10;
				 }else {
					 break;
				 }
			 }
			 if(gen<5000) {
				 dn[gen]=gen;
			 }
			 
			 d1=Integer.parseInt(d)+1;
			 d=String.valueOf(d1);
		}	 
		for(int i=1; i<5000; i++) {
			if(dn[i]==0) {
				System.out.println(n);
				sum+=n;	
			}
			n++;
		}
		System.out.println(sum);
	}

}
