import java.util.Random;
import java.util.Scanner;

public class practice2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] a= new int[5][7];
		int num=1;
		int numnum=0;
		while(numnum<=7) {
		      for(int i=0; i<4; i++) {
		          for(int j=0; j<7; j++) {
		              if((i+j)==numnum) {
		                 a[i][j]=num;
		                 num++;
		              }
		          }
		      }
		      numnum++;
		}
		for(int i=0; i<4; i++) {
		    for(int j=0; j<7; j++) {
		        System.out.print(a[i][j]+"  ");
		    }
		    System.out.println();
		}  
	}	
}
