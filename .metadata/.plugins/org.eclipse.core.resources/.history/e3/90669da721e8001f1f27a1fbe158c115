import java.util.Random;
import java.util.Scanner;

public class practice2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] tempArr={
		          {0,0,1},
		          {1,1,1}, 
		          {1,0,0}
		          };
		int[][]temp2={
				  {0,0,1},
		          {1,1,1}, 
		          {1,0,0}
		          };
		int spin=0;
		int wing1=0;
		int wing2=2;
		int wing3=2;
		int wing4=0;
		while(spin<1) {
		       spin++;
		       for(int i=0; i<2; i++) {
		            tempArr[wing1][i]=temp2[i][wing2];
		       }
		       for(int i=0; i<2; i++) {
		            tempArr[i][wing2]=temp2[wing3][2-i];
		       }
		       for(int i=2; i>0; i--) {
		            tempArr[wing3][i]=temp2[i][wing4];
		       }
		       for(int i=2; i>0; i--) {
		            tempArr[i][wing4]=temp2[wing1][2-i];
		       }
		}  
		for(int i=0; i<3; i++) {
		    for(int j=0; j<3; j++) {
		        System.out.print(tempArr[i][j]+" ");
		    }
		    System.out.println();
		}  
	}	
}
