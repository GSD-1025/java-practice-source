import java.util.ArrayList;

public class practice { 	
	public static void main(String[] args){ 		
	int[][][] test1=new int[11][11][4];
	int num=1;
	for(int i=0; i<11; i++) {
			test1[0][i][3]=num;
			num++;
	}
	for(int i=1; i<11; i++) {
		test1[i][10][3]=num;
		num++;
    }
	for(int i=9; i>=0; i--) {
		test1[10][i][3]=num;
		num++;
    }
	for(int i=10; i>10; i--) {
		test1[i][0][3]=num;
		num++;
    }
	for(int i=0;i<11;i++) {
		for(int j=0; j<11;j++) {
			System.out.print(test1[i][j][3]+" ");
		}
		System.out.println();
	}
	

		
			
		
		 
	} 
}
