import java.util.Random;
import java.util.Scanner;

public class practice2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] tempArr={
		          {1,1,0},
		          {0,1,0}, 
		          {0,1,1}
		          };
		int spin=0;
		int wing1=0;
		int wing2=2;
		int wing3=2;
		int wing4=0;
		while(spin==0) {
		       spin++;
		       for(int i=0; i<2; i++) {
		            tempArr[i][wing2]=tempArr[wing1][i];
		       }
		       for(int i=0; i<2; i++) {
		            tempArr[wing1][i]=tempArr[2-i][wing4];
		       }
		       for(int i=2; i>0; i--) {
		            tempArr[wing3][i]=tempArr[2-i][wing2];
		       }
		       for(int i=0;i<2; i++) {
		            tempArr[wing3][2-i]=tempArr[wing1][i];
		       }
		}  
		for(int i=0; i<2; i++) {
		    for(int j=0; j<2; j++) {
		        System.out.print(tempArr[i][j]+" ");
		    }
		    System.out.println();
		}  
	}	
}
