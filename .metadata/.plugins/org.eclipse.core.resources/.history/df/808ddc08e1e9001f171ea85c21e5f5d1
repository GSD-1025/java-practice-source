import java.util.ArrayList;

public class practice { 	
	static int[][][] test1=new int[11][11][4];
	public static void main(String[] args){ 	
	int num=1;
	for(int i=0; i<11; i++) {
			test1[0][i][3]=num;
			num++;
	}
	for(int i=1; i<11; i++) {
		test1[i][10][3]=num;
		num++;
    }
	for(int i=9; i>=0; i--) {
		test1[10][i][3]=num;
		num++;
    }
	for(int i=9; i>0; i--) {
		test1[i][0][3]=num;
		num++;
    }
	for(int i=0;i<11;i++) {
		for(int j=0; j<11;j++) {
			System.out.print(test1[i][j][3]+" ");
		}
		System.out.println();
	}
	turn();
	System.out.println();
	System.out.println();
	for(int i=0;i<11;i++) {
		for(int j=0; j<11;j++) {
			System.out.print(test1[i][j][3]+" ");
		}
		System.out.println();
	}
	

		
			
		
		 
	} 
    
	public static void turn() {
		int cnt=1;
		int s=0;
		int k=0;
		while(cnt>0) {
			k=test1[0][0][3];
			for(int i=9; i>=s;i--) {
				test1[0][i+1][3]=test1[0][i][3];
				System.out.println(test1[0][i+1][3]);
			}
			for(int i=9; i>=s; i--) {
				test1[i+1][10][3]=test1[i][10][3];
				System.out.println(test1[i+1][10][3]);
			}
			for(int i=s+1; i<10; i++ ) {
				test1[10][i][3]=test1[10][i-1][3];
			}
			for(int i=s+1; i<10; i++) {
				test1[i][10][3]=test1[i-1][10][3];
			}
			
			cnt--;
			
			
			
		}
	}


}

