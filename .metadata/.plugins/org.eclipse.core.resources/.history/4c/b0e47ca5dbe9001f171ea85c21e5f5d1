public class practice2 {
	static String[][][] map= new String[13][13][5];
	static int p1=1;
	static int p2=2;
	static int p3=3;
	static int p4=4;
	static int dice;
	static int duobdice;
	static Random ran= new Random();

	public static void main(String[] args) {
		mapinfo();
		System.out.println();
		mapprint();
		dice=rolldice();
		movingupdate(1);
	}
	public static void mapprint() {
		for(int i=0; i<13; i++) {
			for(int j=0; j<13; j++) {
				System.out.print(map[i][j][0]+" ");
			}
			System.out.println();
			System.out.println();
		}
	}
	public static void mapinfo() {
		for(int i=1; i<11;i++) {
			map[0][i][0]="칸";
			map[12][i+1][0]="칸";
			map[i+1][0][0]="칸";
			map[i][12][0]="칸";
		}
		map[1][1][0]="기부 복권";
		map[11][11][0]="출발";
		map[11][1][0]="무인도";
		map[1][11][0]="우주 여행";
		String[] mapl1= {"대만","기부","중국","필리핀","제주도","싱가포르","기부","이집트","터키"};
		String[] mapl2= {"그리스","기부","덴마크","스웨덴","여객기","스위스","기부","독일","캐나다"};
		String[] mapl3= {"아르헨티나","기부","브라질","기부","부산","하와이","포르투갈","크루즈","스페인"};
		String[] mapl4= {"일본","우주선","프랑스","이탈리아","기부","영국","미국","기부","대한민국"};
		//라인1
		for(int i=0; i<4;i++) {
			for(int j=0; j<9; j++) {
				if(i==0) {
				  map[11][10-j][i]=mapl1[j];
				}else if(map[11][10-j][0].equals("기부")) {
				  map[11][10-j][1]="100000";
				}else if(i==1) {
				  map[11][10-j][i]="200000";
				}else if(i==2) {
				  map[11][10-j][i]=" ";
				}else if(i==3) {
				  map[11][10-j][i]=" ";
				}
			}
		}
		//라인2
		for(int i=0; i<4;i++) {
			for(int j=0; j<9; j++) {
				if(i==0) {
				  map[10-j][1][i]=mapl2[j];
				}else if(map[11][10-j][0].equals("기부")) {
				  map[10-j][1][i]="100000";
				}else if(i==1) {
				  map[10-j][1][i]="300000";
				}else if(i==2) {
				  map[10-j][1][i]=" ";
				}else if(i==3) {
				  map[10-j][1][i]=" ";
				}
			}
		}
		//라인3
		for(int i=0; i<4;i++) {
			for(int j=0; j<9; j++) {
				if(i==0) {
				  map[1][j+2][i]=mapl3[j];
				}else if(map[11][10-j][0].equals("기부")) {
				  map[1][j+2][1]="100000";
				}else if(i==1) {
				  map[1][j+2][i]="400000";
				}else if(i==2) {
				  map[1][j+2][i]=" ";
				}else if(i==3) {
				  map[1][j+2][i]=" ";
				}
			}
		}
		//라인4
		for(int i=0; i<4;i++) {
			for(int j=0; j<9; j++) {
				if(i==0) {
				  map[j+2][11][i]=mapl4[j];
				}else if(map[11][10-j][0].equals("기부")) {
				  map[j+2][11][1]="100000";
				}else if(i==1) {
				  map[j+2][11][i]="500000";
				}else if(i==2) {
				  map[j+2][11][i]=" ";
				}else if(i==3) {
				  map[j+2][11][i]=" ";
				}
			}
		}
	}
	public static int rolldice() {
		duobdice=0;
		int dice1=ran.nextInt(6)+1;
		int dice2=ran.nextInt(6)+1;
		if(dice1==dice2) {
			duobdice=1;
		}
		return dice1*dice2;
	
	}
	public static void movingupdate(int a) {
		int wing1=0,wing2=0,wing3=0,wing4=0;
		int move=dice;
		int index=0;
		int cnt=0;
		for(int i=0; i<10; i++) {
			cnt++;
			if(map[12][11-i][a].equals("1")) {
				map[12][11-i][a]="s";
				index=11-i;
				move+=cnt;
				wing1++;
				break;
			}	
			else if(map[11-i][0][a].equals("1")) {
				map[11-i][0][a]="s";
				index=11-i;
				move+=cnt;
				wing2++;
				break;
			}
			else if(map[0][i+1][a].equals("1")) {
				map[0][i+1][a]="s";
				index=i+1;
				move+=cnt;
				wing3++;
				break;
			}
			else if(map[i-30][12][a].equals("1")) {
				map[i+1][12][a]="s";
				index=i+1;
				move+=cnt;
				wing4++;
				break;
		   }
		}
		if(move<11) {
			
		}
		while(move>0) {
			if(wing1==1) {
				if(move>11) {
					wing1--;
					wing2++;
					move-=11;
					continue;
				}
			}
			else if(wing1==1) {
				if(move>11) {
					wing2--;
					wing3++;
					move-=11;
					continue;
				}
			}
			else if(wing1==1) {
				if(move>11) {
					wing3--;
					wing4++;
					move-=11;
					continue;
				}
			}
			else if(wing1==1) {
				if(move>11) {
					wing3--;
					wing4++;
					move-=11;
					continue;
				}
			}
		}
		
	}
